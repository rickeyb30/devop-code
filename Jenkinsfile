pipeline {
  agent any
  stages {
    stage ('Build') {
      steps {
      echo "build steps"
        sleep 10
      }
    } 
    stage ('Test') {
      steps {
      echo "test steps"
        sleep 10
      }
    } 
    stage ('Deploy') {
      steps {
      echo "deploy steps"
        sleep 10
      }
    } 
    
    stage ('docker') {
      steps {
      echo "image step"
        sleep 10
     }
   } 
  }
}
  
